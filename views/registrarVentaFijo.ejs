
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
  <h2>Vendedor: 
      <% if(user) {%>
          <span class="badge bg-success"> <%= user.nombre %></span>
          <%}%>
    </h2>
  
  <div class="container py-5 h-100">
      <div class="row justify-content-center align-items-center h-100">
        <div class="col-12 col-lg-9 col-xl-12">
          <div class="card shadow-2-strong card-registration" style="border-radius: 15px;">
            <div class="card-body p-4 p-md-5">
              <h3 class="mb-4 pb-2 pb-md-0 mb-md-5">Registration Form</h3>
              <form action="/pruebagoogle" method="POST">
    
                <div class="row">
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="text" id="nombreDelCliente" name="nombreDelCliente" class="form-control form-control-lg" required/>
                      <label class="form-user" for="nombreDelCliente">Nombre del Cliente</label>
                    </div>
    
                  </div>
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="text" id="segundoNombreDelCliente" name="segundoNombreDelCliente" class="form-control form-control-lg"/>
                      <label class="form-user" for="segundoNombreDelCliente">Segundo Nombre del Cliente</label>
                    </div>
    
                  </div>
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="text" id="primerApellidoDelCliente" name="primerApellidoDelCliente" class="form-control form-control-lg"required/>
                      <label class="form-user" for="primerApellidoDelCliente">Primer Apellido del Cliente</label>
                    </div>
    
                  </div>
                </div>
    
                <div class="row">
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="text" id="segundoApellidoDelCliente" name="segundoApellidoDelCliente" class="form-control form-control-lg"required/>
                      <label class="form-user" for="segundoApellidoDelCliente">Segundo Apellido del Cliente</label>
                    </div>
    
                  </div>
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <select class="form-select form-select-lg" id="tipoDeDocumentoDeIdentidad" name="tipoDeDocumentoDeIdentidad" required>
                         <option value="Identidad">Identidad</option>
                         <option value="DIMEX">DIMEX</option>
                         <option value="Pasaporte">Pasaporte</option>
                         <option value="Carnet_de_Refugio">Carnet de Refugio</option>
                         <option value="Permiso_Laboral">Permiso Laboral</option>
                      </select>
                      <label class="form-label" for="nombre">Tipo De Documento De Identidad</label>
                   </div>
                      
                  </div>
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="number" id="numeroDeDocumento" name="numeroDeDocumento" class="form-control form-control-lg"required/>
                      <label class="form-user" for="numeroDeDocumento">Numero De Documento</label>
                    </div>
    
                  </div>
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <select class="form-select form-select-lg" id="nacionalidad" name="nacionalidad" required>
                         <option value="Costarricense">Costarricense</option>
                         <option value="Nicaraguense">Nicaraguense</option>
                         <option value="Venezolano">Venezolano</option>
                         <option value="Colombiano">Colombiano</option>
                         <option value="Otro">Otro</option>
                      </select>
                      <label class="form-label" for="nombre">Tipo De Documento De Identidad</label>
                   </div>
                      
                  </div>
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="number" id="numeroCelularDeTramite" name="numeroCelularDeTramite" class="form-control form-control-lg"required/>
                      <label class="form-user" for="numeroCelularDeTramite">Numero Celular De Tramite</label>
                    </div>
    
                  </div>
    
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="number" id="numeroDeContacto1" name="numeroDeContacto1" class="form-control form-control-lg" required/>
                      <label class="form-user" for="numeroDeContacto1">Numero De Contacto 1</label>
                    </div>
    
                  </div>
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="number" id="numeroDeContacto2" name="numeroDeContacto2" class="form-control form-control-lg" />
                      <label class="form-user" for="numeroDeContacto2">Numero De Contacto 2</label>
                    </div>
    
                  </div>
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <select class="form-select form-select-lg" id="tipoDeTramite" name="tipoDeTramite" required>
                         <option value="Standalone">Standalone</option>
                         <option value="Doble Play">Doble Play</option>
                         <option value="Triple Play">Triple Play</option>
                         <option value="Liberty Total (Sin Television)">Liberty Total (Sin Television)</option>
                         <option value="Liberty Total (Con Television)">Liberty Total (Con Television)</option>
                      </select>
                      <label class="form-label" for="nombre">Tipo De Tramite</label>
                   </div>
                      
                  </div>

                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <div id="StandaloneSelect">

                        <select class="form-select form-select-lg" id="StandaloneDrop" name="StandaloneDrop" required>
                          <option value="Mega 30">Mega 30</option>
                          <option value="Mega 100">Mega 100</option>
                          <option value="Mega 200">Mega 200</option>
                          <option value="Mega 325">Mega 325</option>
                          <option value="Mega 450 HBO MAX">Mega 450 HBO MAX</option>
                          <option value="Mega 300 HBO MAX">Mega 300 HBO MAX</option>
                          <option value="Mega 150 HBO MAX">Mega 150 HBO MAX</option>
                       </select>
                       <label class="form-label" for="nombre">Tipo De Tramite</label>

                      </div>
                   </div>
                      
                  </div>
                  
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <div id="DoblePlaySelect">
                      <select class="form-select form-select-lg" id="StandaloneDrop" name="StandaloneDrop" required>
                         <option value="Mega 30">Mega 30</option>
                         <option value="Mega 100">Mega 100</option>
                         <option value="Mega 200">Mega 200</option>
                         <option value="Mega 325">Mega 325</option>
                         <option value="Mega 450 HBO MAX">Mega 450 HBO MAX</option>
                      </select>
                      <label class="form-label" for="nombre">Tipo De Tramite</label>
                    </div>
                   </div>
                      
                  </div>
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <select class="form-select form-select-lg" id="tipoDePlanAContratar" name="tipoDePlanAContratar" required>
                        <option value="@1Plus">@1Plus</option> 
                        <option value="@1">@1</option>
                         <option value="@2">@2</option>
                         <option value="@3">@3</option>
                         <option value="@4">@4</option>
                         <option value="@5">@5</option>
                         <option value="@6">@6</option>
                      </select>
                      <label class="form-label" for="nombre">Tipo De Plan A Contratar</label>
                   </div>
                      
                  </div>
    
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="text" id="direccionExacta" name="direccionExacta" class="form-control form-control-lg" required/>
                      <label class="form-user" for="direccionExacta">Direccion Exacta</label>
                    </div>
    
                  </div>
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <select class="form-select form-select-lg" id="provincia" name="provincia" required>
                         <option value="San_Jose">San Jose</option>
                         <option value="Heredia">Heredia</option>
                         <option value="Alajuela">Alajuela</option>
                         <option value="Cartago">Cartago</option>
                         <option value="Limon">Limon</option>
                         <option value="Guanacaste">Guanacaste</option>
                         <option value="Puntarenas">Puntarenas</option>
                      </select>
                      <label class="form-label" for="nombre">Provincia</label>
                   </div>
                      
                  </div>
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="text" id="canton" name="canton" class="form-control form-control-lg" required/>
                      <label class="form-user" for="canton">Canton</label>
                    </div>
    
                  </div>
    
                  <div class="col-md-4 mb-4">
    
                    <div class="form-outline">
                      <input type="text" id="distritp" name="distritp" class="form-control form-control-lg" required/>
                      <label class="form-user" for="distritp">Distrito</label>
                    </div>
    
                  </div>
  
                  <div class="col-md-4 mb-4">
    
                      <div class="form-outline">
                        <div class="prueba">
                        <input type="text" id="nombreVendedor" name="nombreVendedor" value="<%= user.nombre %>" readonly class="form-control form-control-lg" />
                        <label class="form-user" for="nombreVendedor" >Nombre Vendedor</label>
                        </div>
                      </div>
      
                    </div>
    
    
                </div>
    
               
    
                <div class="mt-4 pt-2">
                  <input class="btn btn-primary btn-lg" type="submit" value="Guardar" />
                </div>
    
              </form>
            </div>
          </div>
        </div>
      </div>
    </div> 